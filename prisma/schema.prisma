// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PotentialMatchup {
  id         String @id @default(uuid())
  eventId    String
  gameDate   String
  gameTime   String
  name       String
  league     String
  gameStatus String
}
